<%# <%= simple_form_for(restaurant) do |f| %>
<%= simple_form_for(@shelter) do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :capacity %>
  <%= f.input :c_logo, as: :file, label: "Shelter Logo: "%>
  <%= f.input :c_banner, as: :file, label: "Shelter Banner: " %>
  <%= f.input :description %>
  <%= f.submit %>
<% end %>
